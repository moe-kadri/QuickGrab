@model User
<!DOCTYPE html>
<html leng="en">
<head>
    <meta charset="UTF-8">
    <title>Profile Page</title>
    <environment>
        <link rel="stylesheet" href="~/css/Home Page CSS.css">
        <link rel="stylesheet" href="~/css/Profile CSS.css"> 
    </environment>
    
    <script src="https://kit.fontawesome.com/cacf32ff12.js" crossorigin="anonymous"></script>
</head>
<body>



    <div class="container">
        <div class="leftbox">
            
        </div>
        <div class="rightbox">
            <div class="profile tabShow">
                <h1>Personal Information</h1>
                <h2>Name</h2>
                <input type="text" class="input" value="@Model.UserName" readonly>
                <h2>Phone</h2>
                <input type="text" value="@Model.PhoneNumber" readonly class="input" >
                <h2>Email</h2>
                <input type="email" value="@Model.Email" readonly class="input">
            
            </div>
           </div>
    </div>

    <script src="~/js/Profile JS.js"></script>

</body>
</html>